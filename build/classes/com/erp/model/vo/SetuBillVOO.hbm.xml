		</property>
		<property name="compName" type="string">
			<column name="comp_name" length="30" />
		</property>
		<property name="compTime" type="timestamp">
			<column name="comp_time" />
		</property>
		<property name="enab" type="string">
			<column name="enab" length="1" />
		</property>
		<property name="disaId" type="long">
			<column name="disa_id" not-null="true" />
		</property>
		<property name="updaTime" type="timestamp">
			<column name="upda_time" not-null="true" />
		</property>
