		<property name="needWeld" type="string">
			<column name="need_weld" length="1" not-null="true" />
		</property>
		<property name="needStee" type="string">
			<column name="need_stee" length="1" not-null="true" />
		</property>
		
		<property name="creaId" type="long">
			<column name="crea_id" not-null="true" />
		</property>
		<property name="creaName" type="string">
			<column name="crea_name" length="30" />
		</property>
		<property name="creaTime" type="timestamp">
			<column name="crea_time" />
		</property>
		<property name="exam" type="string">
			<column name="exam" length="1" />
		</property>
		<property name="examId" type="long">
			<column name="exam_id" not-null="true" />
		</property>
		<property name="examName" type="string">
			<column name="exam_name" length="30" />
		</property>
		<property name="examTime" type="timestamp">
			<column name="exam_time" />
